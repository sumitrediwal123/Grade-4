# Task: Draw star 2 and star 3 at different locations

import turtle

#draw moon:
turtle.bgcolor("black")  #Background color
turtle.penup()
turtle.goto(180, 180)
turtle.pendown()
turtle.color("white")
turtle.begin_fill()
turtle.circle(25)
turtle.end_fill()

#draw stars:
#star 1
turtle.penup()
turtle.goto(40, 180)
turtle.pendown()
turtle.color("yellow")
turtle.begin_fill()
for i in range(5):
    turtle.forward(20)   
    turtle.right(144)
turtle.end_fill()

#star 2






#star 3
